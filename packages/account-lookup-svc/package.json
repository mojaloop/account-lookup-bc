{
  "name": "@mojaloop/account-lookup-bc-svc",
  "version": "0.0.1",
  "description": "Account Lookup BC service.",
  "license": "Apache-2.0",
  "homepage": "https://github.com/mojaloop/account-lookup-bc/tree/main/packages/account-lookup-svc",
  "bugs": {
    "url": "https://github.com/mojaloop/account-lookup-bc/issues#readme"
  },
  "contributors": [
    "Gon√ßalo Garcia <goncalogarcia99@gmail.com>",
    "Jason Bruwer <jason.bruwer@coil.com>",
    "Jose Antunes <jose.antunes@arg.software>",
    "Pedro Sousa Barreto <pedrob@crosslaketech.com>",
    "Rui Rocha <rui.rocha@arg.software>"
  ],
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "start": "npm run service",
    "service": "tsc && node dist/index.js",
    "build": "tsc",
    "clean:dist": "rm -rf dist",
    "clean:node-modules": "rm -rf node_modules",
    "clean:dist-and-node-modules": "rm -rf dist node_modules",
    "clean": "npm run clean:dist",
    "audit:check": "npm npm audit",
    "dep:check": "ncu -e 2",
    "dep:update": "ncu -u",
    "publish": "npm version patch && npm publish --tag=latest --access public",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "test:unit": "jest --passWithNoTests  --testMatch **/test/unit/*.test.ts || ECHO.",
    "test:integration": "jest --passWithNoTests --testMatch **/test/integration/**/*.test.ts",
    "test:coverage": "jest --passWithNoTests --coverage --coverageThreshold='{}' --testMatch **/test/unit/**/*.test.ts",
    "test:coverage-check": "jest --coverage --testMatch **/test/unit/**/*.test.ts"
  },
  "dependencies": {
    "@mojaloop/account-lookup-bc-client": "*",
    "@mojaloop/account-lookup-bc-domain": "*",
    "@mojaloop/account-lookup-bc-infrastructure": "*",
    "@mojaloop/auditing-bc-client-lib": "^0.0.6",
    "@mojaloop/auditing-bc-public-types-lib": "^0.0.7"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "eslint": "^8.17.0",
    "express": "^4.18.1",
    "jest": "^28.1.1",
    "npm-check-updates": "^13.1.2",
    "ts-jest": "^28.0.4",
    "typescript": "^4.7.3"
  }
}
