        "@mojaloop/account-lookup-bc-implementations-lib": "*",
        "@opentelemetry/api": "~1.8.0"
    },
    "devDependencies": {
        "@mojaloop/account-lookup-bc-shared-mocks-lib": "*",
        "@types/express": "^4.17.21",
        "eslint": "^8.57.0",
        "express": "^4.19.2",
        "express-validator": "^7.1.0",
        "jest": "^29.7.0",
        "npm-check-updates": "^16.14.20",
        "ts-jest": "^29.1.4",
        "typescript": "^5.1.0",
        "tslib": "~2.6.2"
    },
    "engines": {
        "node": ">=20.10.0"
    }
